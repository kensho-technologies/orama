{
  "name": "explorer",
  "version": "0.1.0",
  "description": "",
  "main": "src/components/App.js",
  "scripts": {
    "start": "npm prune && npm install",
    "test": "npm run lint && npm run test:run",
    "build": "npm run build:lib && npm run build:webpack",
    "dev-server": "webpack-dev-server --hot --inline --display-error-details --colors --config webpack.config.js --content-base ./dist",
    "clean": "rimraf lib dist/bundle.js",
    "build:lib": "babel src --out-dir lib",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.js",
    "lint": "eslint src",
    "test:run": "NODE_ENV=test mocha --compilers js:babel/register --reporter dot ./src/**/*-test.js",
    "test:watch": "npm run test:run -- --watch",
    "test:cov": "babel-node ./node_modules/.bin/isparta cover --report text --report html ./node_modules/.bin/_mocha -- --reporter dot ./src/**/*-test.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Luis Carli",
  "license": "MIT",
  "devDependencies": {
    "babel": "5.8.19",
    "babel-core": "5.8.19",
    "babel-eslint": "4.0.5",
    "babel-loader": "5.3.2",
    "eslint": "0.23",
    "eslint-config-airbnb": "0.0.6",
    "eslint-plugin-react": "3.1.0",
    "expect": "1.8.0",
    "isparta": "3.0.3",
    "mocha": "2.2.5",
    "react-hot-loader": "1.2.8",
    "rimraf": "2.4.2",
    "webpack": "1.10.5",
    "webpack-dev-server": "1.10.1"
  },
  "dependencies": {
    "react": "0.13.3"
  }
}
