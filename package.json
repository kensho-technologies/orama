{
  "name": "orama",
  "version": "0.0.0",
  "description": "Chart components for React",
  "main": "src/components/App.js",
  "scripts": {
    "prestart": "npm prune && npm update",
    "start": "parallelshell 'node devServer.js' 'npm run test:watch' 'npm outdated'",
    "clean": "rimraf lib dist/bundle.js",
    "prebuild": "npm test && npm run clean",
    "build": "npm run build:lib && npm run build:webpack",
    "postbuild": "npm run doc",
    "build:lib": "NODE_ENV=production babel src --out-dir lib",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "test": "NODE_ENV=test mocha --compilers js:babel/register -R dot 'src/**/*.test.js'",
    "posttest": "eslint src",
    "test:watch": "onchange src -- npm test",
    "cov": "NODE_ENV=test babel-node node_modules/.bin/isparta cover --report text --report lcovonly --report html node_modules/.bin/_mocha -- -R dot 'src/**/*.test.js'",
    "postcov": "open coverage/index.html",
    "doc": "npm run doc:jsdoc && npm run doc:react",
    "doc:jsdoc": "jsdoc src -r -c .jsdocrc -t templates/haruki -d console | babel-node scripts/jsdocToMd.js > docs/functions.md",
    "doc:react": "react-docgen src/components | babel-node scripts/reactdocToMd.js > docs/components.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kensho/orama"
  },
  "author": "Luis Carli",
  "license": "MIT",
  "devDependencies": {
    "@luiscarli/shallow-render": "^1.0.0",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "eslint": "^1.7.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.6.3",
    "express": "^4.13.3",
    "isparta": "^3.1.0",
    "jsdoc": "^3.3.3",
    "jsdoc-babel": "^0.1.0",
    "mocha": "^2.3.3",
    "onchange": "^2.0.0",
    "parallelshell": "^2.0.0",
    "react-addons-test-utils": "^0.14.0",
    "react-docgen": "^2.3.1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1"
  },
  "dependencies": {
    "d3": "^3.5.6",
    "d3-request": "^0.2.3",
    "d3-scale": "^0.2.0",
    "d3-time-format": "^0.2.0",
    "lodash": "^3.10.1",
    "on-update": "^1.0.0",
    "ramda": "^0.18.0",
    "react": "^0.14.0",
    "react-display": "^1.0.0",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.0",
    "react-dom": "^0.14.0"
  },
  "peerDependencies": {
    "react": "^0.14.0"
  },
  "engines": {
    "node": ">=0.12.0"
  }
}
