{
  "name": "orama",
  "version": "0.1.0",
  "description": "Chart components for React",
  "main": "src/components/App.js",
  "scripts": {
    "prestart": "npm prune && npm update",
    "start": "parallelshell 'node devServer.js' 'npm run test:watch' 'npm outdated'",
    "clean": "rimraf lib dist/bundle.js",
    "build": "npm run lib && npm run bundle",
    "lib": "NODE_ENV=production babel src --out-dir lib",
    "bundle": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "test": "NODE_ENV=test mocha --compilers js:babel-register -R dot 'src/**/*.test.js' && eslint src",
    "test:watch": "onchange src -- npm test",
    "cov": "NODE_ENV=test babel-node node_modules/.bin/isparta cover --report text --report lcovonly --report html node_modules/.bin/_mocha -- -R dot 'src/**/*.test.js'",
    "postcov": "open coverage/index.html",
    "publish": "node scripts/publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kensho/orama"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "author": "Luis Carli",
  "license": "MIT",
  "devDependencies": {
    "@luiscarli/shallow-render": "^1.0.0",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "colors": "^1.1.2",
    "eslint": "^1.7.3",
    "eslint-config-airbnb": "^3.0.0",
    "eslint-plugin-react": "^3.6.3",
    "express": "^4.13.3",
    "github": "^0.2.4",
    "isparta": "^4.0.0",
    "mocha": "^2.3.3",
    "onchange": "^2.0.0",
    "parallelshell": "^2.0.0",
    "prompt": "^0.2.14",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1"
  },
  "dependencies": {
    "d3-request": "^0.2.3",
    "d3-scale": "^0.4.0",
    "d3-time-format": "^0.2.0",
    "lodash": "^3.10.1",
    "on-update": "^1.0.0",
    "react-display": "^1.0.0",
    "react-dnd-html5-backend": "^2.0.0",
    "react-dnd": "^2.0.2",
    "react-dom": "^0.14.0",
    "react": "^0.14.0"
  },
  "engines": {
    "node": ">=5.0.0"
  }
}
