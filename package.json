{
  "name": "explorer",
  "version": "0.0.0",
  "description": "DataVis exploration components",
  "main": "src/components/App.js",
  "scripts": {
    "prestart": "npm prune && npm install",
    "start": "parallelshell 'node server.js' 'npm run test:watch' 'npm outdated'",
    "clean": "rimraf lib dist/bundle.js",
    "prebuild": "npm test && npm run clean",
    "build": "npm run build:lib && npm run build:webpack",
    "postbuild": "npm run doc",
    "build:lib": "NODE_ENV=production babel src --out-dir lib",
    "build:webpack": "webpack --config webpack.config.production.js",
    "lint": "eslint src",
    "test": "NODE_ENV=test mocha --compilers js:babel/register -R dot 'src/**/*.test.js'",
    "posttest": "eslint src",
    "test:watch": "onchange src -- npm test",
    "test:cov": "NODE_ENV=test babel-node node_modules/.bin/isparta cover --report text --report lcovonly --report html node_modules/.bin/_mocha -- -R dot 'src/**/*.test.js'",
    "posttest:cov": "open coverage/index.html",
    "doc": "npm run doc:jsdoc && npm run doc:react",
    "doc:jsdoc": "jsdoc src -r -c .jsdocrc -t templates/haruki -d console | babel-node src/scripts/jsdocToMd.js > docs/functions.md",
    "doc:react": "react-docgen src/components | babel-node src/scripts/reactdocToMd.js > docs/components.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kensho/explorer"
  },
  "author": "Luis Carli",
  "license": "MIT",
  "devDependencies": {
    "babel": "5.8.23",
    "babel-core": "5.8.24",
    "babel-eslint": "4.1.2",
    "babel-loader": "5.3.2",
    "babel-plugin-react-transform": "1.0.4",
    "coveralls": "2.11.4",
    "d3-arrays": "0.0.4",
    "eslint": "1.4.3",
    "eslint-config-airbnb": "0.0.8",
    "eslint-plugin-react": "3.3.2",
    "express": "4.13.3",
    "isparta": "3.0.4",
    "jsdoc": "3.3.2",
    "jsdoc-babel": "0.1.0",
    "mocha": "2.3.2",
    "onchange": "2.0.0",
    "parallelshell": "2.0.0",
    "react-addons-test-utils": "0.14.0-rc1",
    "react-docgen": "2.1.2",
    "react-transform-catch-errors": "0.1.3",
    "react-transform-webpack-hmr": "0.1.4",
    "redbox-react": "1.0.5",
    "rimraf": "2.4.3",
    "webpack": "1.12.2",
    "webpack-dev-middleware": "1.2.0",
    "webpack-hot-middleware": "2.1.0"
  },
  "dependencies": {
    "d3": "3.5.6",
    "d3-scale": "0.1.1",
    "d3-xhr": "0.1.1",
    "jsxstyle": "0.0.17",
    "lodash": "3.10.1",
    "ramda": "0.17.1",
    "react": "0.14.0-rc1",
    "react-dnd": "1.1.7",
    "react-dom": "0.14.0-rc1",
    "simple-statistics": "1.0.0"
  },
  "engines": {
    "node": ">=0.12.0"
  }
}
